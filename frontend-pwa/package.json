  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "ngrok": "^5.0.0-beta.2",
    "tailwindcss": "^4",
